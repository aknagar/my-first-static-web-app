{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "/_framework/*", "/css/*", "/img/*.{png,jpg,gif}" ]
  },
  "routes": [{
    "route": "/*",
    "allowedRoles": ["authenticated"]
  }],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  }
  }